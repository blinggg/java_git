		System.out.print("메뉴선택>");
		int menu=s.nextInt();
		
		switch(menu) {
		
		case 1:
			System.out.println("============");
			System.out.println("학생입력");
			System.out.println("============");
			break;
		case 2:
			break;
		
		case 3:
			break;
		
		case 4:
			break;
		
		case 5:
			break;
		
		case 6:
			break;
		
		
		}
		
	}
}
