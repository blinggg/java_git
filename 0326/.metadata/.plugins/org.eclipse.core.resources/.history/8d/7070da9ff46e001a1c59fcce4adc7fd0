package ex01;

public class Sample {

	public static void main(String[] args) {
		//배열에 있는 데이터들에 대한 평균값과 최대값 구하기
		int[] num= {23,60,97,65,45,32,78,54};
		int sum=0;
		
			for(int i=0; i<=7; i++) {
				sum=sum+num[i]; 
		
									}//for
			double avg=sum/num.length;
			
			System.out.println("평균값은"+avg+"입니다.");
		
	}//main
}//class
