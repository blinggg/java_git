package ex02;

public class Car {
//필드선언하기----------------------------------------------------------
	String company;
	int maxSpeed;
	
//기본생성자(기본)-------------------------------------------------------
	Car(){
	
	}
	
//메소드(실행)----------------------------------------------------------
	void printCar() {
		System.out.println("제조사:"+company);
		System.out.println("최대속도:"+maxSpeed);
	}
}
