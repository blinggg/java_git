package ex01;

import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		//이름, 국어, 영어, 수학 키보드에서 입력받기
		//총점, 평균 출력
		
		Scanner s=new Scanner(System.in);
		System.out.print("이름은");
		String name=s.next();
		
		System.out.print("국어점수는");
		int kor=s.nextInt();
		
		System.out.print("영어점수는");
		int eng=s.nextInt();
		
		System.out.print("수학점수는");
		int mat=s.nextInt();
		
		
		
		
				

		
	}

}
