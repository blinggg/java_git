package ex01;

public class Car {
	//car1필드
	String company="현대";
	String model;     	//차종
	String color;		
	int maxSpeed;
	
	//출력메소드 (동작)
	void printCar(Car car1) {
		System.out.println("-----자동자 정보-----");
		System.out.println("회사"+car1.company);
		System.out.println("모델:"+car1.model);
		System.out.println("최대속도:"+car1.maxSpeed);
		
	}
	
	
	
}//c
